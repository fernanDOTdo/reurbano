{% extends '::frontend.html.twig' %}

{% block content %}
<div class="content box grid_12 round_all">
    <div class="grid_7 prefix_1 alpha">
        <h1>{{ title }}</h1>
        <form class="block" action="{{ path('core_html_contact') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
                <div><button class="button green m10"><span>Enviar</span></button> <a class="button red" href="#" onclick="javascript:history.go(-1); return false;">Cancelar</a></div>
        </form>
    </div>
    <div class="grid_4 omega">
    {% render 'ReurbanoCoreBundle:Widget\\Block:render' %}
    </div>
</div>
<div class='clear'></div>
{% endblock content %}  